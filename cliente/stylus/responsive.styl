@media screen and (min-width:780px)
  #barraNavegacion
    display none
  .inicioSession
    div
      margin-top 10%
      width 15%
  nav
    -webkit-transform: translate(0, 0);
    transform: translate(0, 0);
    position fixed
    top 0
    z-index 800
    background black
    //position relative
    width 100%
    border-radius 0
    padding 0em 0 0em 0
    //padding 0
    //border 1px solid red
    left 0
    right 0
    top 0
    bottom initial
    box-shadow 0px 0px 0px #888
    #cerrarMenu
      display none
    div
      margin 0
      margin-right  0em
      padding 0
      display inline-block
      a:hover
        //border-bottom 3px solid #666
        color #ccc

      a
        font-size 1.3em
        margin 0
        border-right 1px solid #888
        border-left 1px solid #888
        color white
        //margin-left 0.4em
        //border 1px solid red
        border-radius 0
        background black
        //border-bottom 3px  solid #888
        display inline-block
        vertical-align top
        padding 0.7em 0.8em 0.5em 0.8em
        //width 33%

  .contenido
    top 3em
    width 68%
    margin-left 3%

  .contenidoLateral
    top 3em
    width 25%
    padding 1em 1% 1em 1%
    //margin-left 2%
    height 94%
    position fixed
    right 0
    margin 0
    .cuadroImagen
      padding 0em
      //height 6em
      border-radius 5px
      figure
        position relative
        width 7em
        height 7em
        //margin 1em 0 1em 1em
        display inline-block
        //border 1px solid blue
        //margin 0
        //padding 0
        //border 1px solid red
        //border-radius 50%
      .nombreProfesion
        //border 1px solid blue
        width 90%
        margin 0em auto 1.5em auto
        display inline-block
        //width 47%
  .itemListIzq
    margin 0
    //border 1px solid red
    width 25%
  .itemListDer
    margin 0
    width 70%
    padding 0% 2% 0 2%
  .itemListIzq, .itemListDer, .panelIzq, .panelDer
    margin 0
    overflow-y scroll
    position absolute
    top 3.5em
