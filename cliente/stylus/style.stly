    color #ccc
    font-size 1.2em


    border 1px solid #ccc
.itemListIzq
  width 25%
  padding 0 0 0 0
  .itemList
    border-bottom 1px solid #ccc
    a
      padding 5% 0 5% 2%
      text-decoration none
      display inline-block
      width 100%
.itemListDer
  width 70%
  right 0
  padding 0% 2% 0 2%
.panelIzq
  //border 1px solid red
  width 49%
  textarea
    outline none
    box-shadow none
    font-size 1.2em
    display inline-block
    position absolute
    top 0
    color #3c4043
    line-height 2.5rem
    resize none
    //white-space nowrap
    border 0
    word-wrap normal
    width 100%
    bottom 0
    left 0
    right 0
    overflow auto
    overflow-x hidden
.panelDer
  width 49%
  border 1px solid blue
  right 0
.itemListIzq, .itemListDer, .panelIzq, .panelDer
  position absolute
  overflow-y scroll
  display inline-block
  top 3.5em
  overflow-x hidden
  bottom 2.5em
  vertical-align top
