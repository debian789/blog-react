{
  "name": "blog-react",
  "version": "1.0.0",
  "description": "Blog implementando react",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cp-static": "cp -r  cliente/static/ public/static",
    "dist": "babel server/server.js server/**/*.js  --out-dir dist --presets es2015,react",
    "generarComponentePublico": "NODE_PATH=. browserify -t [ babelify --presets [ es2015 react ]  ] cliente/router-publico.js -o public/bundle.js",
    "generarComponentePrivado": "NODE_PATH=. browserify -t [ babelify --presets [ es2015 react ]  ] cliente/router-privado.js -o public/bundle-admin.js",
    "server2": "NODE_PATH=. babel-node  server/server --presets es2015,react ",
    "server": "NODE_PATH=./dist node dist/server/server.js",
    "stylusGenerar": "stylus -c cliente/stylus/style.stly -o public/static/css",
    "stylus": "stylus -w -c cliente/stylus/style.stly -o public/static/css",
    "start": " npm run generarComponentePublico && npm run generarComponentePrivado && npm run stylusGenerar && npm run dist && npm run server2"
  },
  "author": "Miguel Suescun <debian789@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.3.13",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.14.2",
    "browserify": "^12.0.1",
    "client-request": "^1.0.1",
    "cookie-parser": "^1.4.0",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "highlight.js": "^9.0.0",
    "history": "^1.13.1",
    "hjs": "0.0.6",
    "marked": "^0.3.5",
    "markedify": "0.0.2",
    "mongoose": "^4.2.9",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^3.1.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.2",
    "reactify": "^1.1.1",
    "require-globify": "^1.3.0",
    "stylus": "^0.52.4"
  }
}
